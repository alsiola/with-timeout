{
    "defaultSeverity": "error",
    "extends": ["tslint:recommended"],
    "jsRules": {},
    "rules": {
        "indent": [true, "spaces", 4],
        "quotemark": [true, "double"],
        "interface-name": [false],
        "trailing-comma": [
            true,
            {
                "multiline": "never",
                "singleline": "never"
            }
        ],
        "arrow-parens": false
    },
    "rulesDirectory": []
}
